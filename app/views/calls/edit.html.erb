<h1>Editing call</h1>

<% form_for(@call) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :cid %><br />
    <%= f.text_field :cid %>
  </p>
  <p>
    <%= f.label :queu_id %><br />
    <%= f.text_field :queu_id %>
  </p>
  <p>
    <%= f.label :queue_name %><br />
    <%= f.text_field :queue_name %>
  </p>
  <p>
    <%= f.label :uniqueid %><br />
    <%= f.text_field :uniqueid %>
  </p>
  <p>
    <%= f.label :agent_id %><br />
    <%= f.text_field :agent_id %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @call %> |
<%= link_to 'Back', calls_path %>
